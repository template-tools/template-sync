{
  "name": "npm-template-sync",
  "version": "0.0.0-semantic-release",
  "description": "Keep npm package in sync with its template",
  "main": "dist/npm-template-sync.js",
  "module": "src/npm-template-sync.js",
  "scripts": {
    "start": "node --harmony-async-await webhook/index",
    "cover": "rollup -c tests/rollup.config.js && mocha --reporter tests/istanbul.reporter.js build/bundle.test.js",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "prepublish": "rollup --output=bin/npm-template-sync -c -- src/npm-template-sync.js && chmod +x bin/npm-template-sync",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "markdown-doctest",
    "test": "mocha --reporter tests/istanbul.reporter.js build/bundle.test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "bin": {
    "npm-template-sync": "bin/npm-template-sync"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/npm-template-sync.git"
  },
  "engines": {
    "node": ">=6.9.3"
  },
  "dependencies": {
    "caporal": "^0.4.0",
    "deep-extend": "^0.4.1",
    "expression-expander": "^5.3.9",
    "github-basic": "^2.1.0",
    "github-webhook-handler": "^0.6.0",
    "js-yaml": "^3.8.2",
    "keychain": "^1.3.0",
    "micro": "latest",
    "now-logs": "0.0.7",
    "octonode": "^0.7.9",
    "prompt": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cracks": "3.1.2",
    "cz-conventional-changelog": "^2.0.0",
    "istanbul": "^0.4.5",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "mocha": "^3.2.0",
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-istanbul": "^1.1.0",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "semantic-release": "^6.3.6"
  },
  "release": {},
  "keywords": [
    "npm",
    "template"
  ],
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/npm-template-sync/issues"
  },
  "homepage": "https://github.com/arlac77/npm-template-sync#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ]
}
