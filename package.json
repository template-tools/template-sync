{
  "name": "npm-template-sync",
  "version": "0.0.0-semantic-release",
  "description": "Keep npm package in sync with its template",
  "main": "dist/npm-template-sync.js",
  "module": "src/npm-template-sync.js",
  "scripts": {
    "cover": "nyc ava",
    "test": "ava",
    "pretest": "rollup -c rollup.config.test.js",
    "precover": "rollup -c rollup.config.test.js",
    "posttest": "npm run prepublish && markdown-doctest",
    "start": "node --harmony-async-await webhook/index",
    "docs": "jsdoc2md --configure doc/jsdoc.json -l off -t doc/README.hbs -f src/*.js >README.md",
    "prepublish": "rollup --output=bin/npm-template-sync -c -- src/npm-template-sync.js && chmod +x bin/npm-template-sync",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "bin": {
    "npm-template-sync": "bin/npm-template-sync"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/npm-template-sync.git"
  },
  "engines": {
    "node": ">=8.1.0"
  },
  "dependencies": {
    "caporal": "^0.6.0",
    "deep-extend": "^0.5.0",
    "expression-expander": "^5.3.11",
    "github-basic": "^2.3.0",
    "github-webhook-handler": "^0.6.0",
    "js-yaml": "^3.8.4",
    "keychain": "^1.3.0",
    "micro": "^7.3.3",
    "now-logs": "0.0.7",
    "octonode": "^0.7.13",
    "ora": "^1.2.0",
    "prompt": "^1.0.0",
    "semver-diff": "^2.1.0"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "cz-conventional-changelog": "^2.0.0",
    "jsdoc-babel": "^0.3.0",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.0.1",
    "rollup": "^0.42.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-istanbul": "^1.1.0",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "semantic-release": "^6.3.6",
    "cracks": "3.1.2",
    "xo": "^0.19.0",
    "babel-preset-stage-3": "^6.24.1"
  },
  "release": {},
  "keywords": [
    "npm",
    "template"
  ],
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/npm-template-sync/issues"
  },
  "homepage": "https://github.com/arlac77/npm-template-sync#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "ava": {
    "files": [
      "tests/build/bundle.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "tests/build/bundle.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  },
  "xo": {
    "space": true
  }
}
