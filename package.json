{
  "name": "npm-template-sync",
  "version": "0.0.0-semantic-release",
  "description": "Keep npm package in sync with its template",
  "main": "dist/npm-template-sync.js",
  "module": "src/npm-template-sync.js",
  "scripts": {
    "cover": "nyc ava",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "precover": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "start": "node --harmony-async-await webhook/index",
    "docs": "jsdoc2md --configure doc/jsdoc.json -l off -t doc/README.hbs -f src/*.js >README.md",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "prepare": "rollup -c -- src/npm-template-sync.js && chmod +x bin/npm-template-sync"
  },
  "bin": {
    "npm-template-sync": "bin/npm-template-sync"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/npm-template-sync.git"
  },
  "engines": {
    "node": ">=8.4.0"
  },
  "dependencies": {
    "caporal": "^0.7.0",
    "deep-extend": "^0.5.0",
    "expression-expander": "^6.0.0",
    "github-basic": "^6.0.0",
    "github-webhook-handler": "^0.6.0",
    "js-yaml": "^3.9.0",
    "keychain": "^1.3.0",
    "micro": "^9.0.0",
    "now-logs": "0.0.7",
    "ora": "^1.3.0",
    "prompt": "^1.0.0",
    "recast": "^0.12.6",
    "semver-diff": "^2.1.0",
    "set-getter": "^0.1.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-preset-stage-3": "^6.24.1",
    "color-convert": "^1.9.0",
    "cracks": "^3.1.2",
    "jsdoc-babel": "^0.3.0",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.2.0",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.0",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "semantic-release": "^7.0.2",
    "xo": "^0.19.0"
  },
  "release": {},
  "keywords": [
    "npm",
    "template"
  ],
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/npm-template-sync/issues"
  },
  "homepage": "https://github.com/arlac77/npm-template-sync#readme",
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "ava": {
    "files": [
      "build/test-bundle.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/test-bundle.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  },
  "xo": {
    "space": true
  }
}
