{
  "name": "npm-template-sync",
  "version": "0.0.0-semantic-release",
  "main": "dist/npm-template-sync.js",
  "module": "src/npm-template-sync.js",
  "description": "Keep npm package in sync with its template",
  "keywords": [
    "npm",
    "template"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "npm-template-sync": "bin/npm-template-sync"
  },
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "precover": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "docs": "documentation readme src/npm-template-sync.js --section=API",
    "semantic-release": "semantic-release",
    "prepare": "rollup -c && chmod +x bin/npm-template-sync",
    "lint": "documentation lint src/npm-template-sync.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "aggregation-repository-provider": "^1.0.4",
    "bitbucket-repository-provider": "^1.2.4",
    "caporal": "^0.9.0",
    "deep-extend": "^0.5.0",
    "expression-expander": "^6.0.1",
    "github-repository-provider": "^2.0.1",
    "js-yaml": "^3.10.0",
    "jsonpath": "^1.0.0",
    "local-repository-provider": "^1.2.1",
    "micromatch": "^3.1.5",
    "ora": "^1.4.0",
    "p-queue": "^2.3.0",
    "prompt": "^1.0.0",
    "recast": "^0.13.1",
    "set-getter": "^0.1.0",
    "simple-diff": "^1.6.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "documentation": "^5.4.0",
    "markdown-doctest": "^0.9.1",
    "mock-repository-provider": "^3.0.1",
    "nyc": "^11.5.0",
    "rollup": "^0.56.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-node-resolve": "^3.0.3",
    "semantic-release": "^13.1.4",
    "tempy": "^0.2.1",
    "travis-deploy-once": "^4.3.4"
  },
  "optionalDependencies": {
    "keytar": "^4.1.0"
  },
  "engines": {
    "node": ">=8.9.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/npm-template-sync.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/npm-template-sync/issues"
  },
  "homepage": "https://github.com/arlac77/npm-template-sync#readme",
  "release": {},
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
