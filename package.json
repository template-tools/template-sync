{
  "name": "npm-template-sync",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/context.js",
  "module": "src/context.js",
  "description": "Keep npm package in sync with its template",
  "keywords": [
    "npm",
    "template"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "npm-template-sync": "bin/npm-template-sync"
  },
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "precover": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "docs": "documentation readme src/context.js --section=API",
    "semantic-release": "semantic-release",
    "prepare": "rollup -c",
    "lint": "documentation lint src/context.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "aggregation-repository-provider": "^1.2.6",
    "babylon": "^6.18.0",
    "bitbucket-repository-provider": "^1.5.1",
    "caporal": "^0.10.0",
    "deep-extend": "^0.6.0",
    "expression-expander": "^6.1.2",
    "github-repository-provider": "^2.1.9",
    "js-yaml": "^3.12.0",
    "jsonpath": "^1.0.0",
    "local-repository-provider": "^1.2.22",
    "micromatch": "^3.1.10",
    "prompt": "^1.0.0",
    "recast": "^0.15.3",
    "set-getter": "^0.1.0",
    "simple-diff": "^1.6.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "documentation": "^8.1.0",
    "execa": "^0.10.0",
    "markdown-doctest": "^0.9.1",
    "mock-repository-provider": "^3.0.13",
    "nyc": "^13.0.1",
    "rollup": "^0.64.1",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-executable": "^1.3.0",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "semantic-release": "^15.9.8",
    "travis-deploy-once": "^5.0.2"
  },
  "optionalDependencies": {
    "keytar": "^4.1.0"
  },
  "engines": {
    "node": ">=10.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/npm-template-sync.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/npm-template-sync/issues"
  },
  "homepage": "https://github.com/arlac77/npm-template-sync#readme",
  "release": {},
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
